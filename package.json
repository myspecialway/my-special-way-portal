{
  "name": "msw-client",
  "version": "0.0.234",
  "license": "MIT",
  "scripts": {
    "build": "ng build --prod",
    "bump": "yarn version --new-version patch",
    "ci-test": "bash ./scripts/test",
    "docker": "bash ./scripts/docker",
    "docker:push": "bash ./scripts/docker-push",
    "docker:deploy": "bash ./scripts/k8s/deploy",
    "e2e": "testcafe chrome tests/ -s \"tests/eyes/screenshots\"",
    "git:push": "bash ./scripts/git-push",
    "lint": "ng lint",
    "ng": "ng",
    "test": "jest --coverage",
    "tslint": "tslint -p tsconfig.json -c tslint.json '**/*.ts' -e 'node_modules/**' -e 'src/polyfills.ts'",
    "test:watch": "jest --watch --coverage",
    "start": "ng serve",
    "start:local": "ng serve --configuration=local",
    "mock-graphql": "npx json-graphql-server ./src/mocks/assets/db.js",
    "stryker": "npx stryker run",
    "testcafe-live": "testcafe-live chrome tests/",
    "testcafe": "testcafe chrome tests/ -s \"tests/eyes/screenshots\"",
    "format:fix": "pretty-quick --staged",
    "format:check": "prettier --config ./.prettierrc --list-different \"src/app/**/*{.ts, .scss}\""
  },
  "private": true,
  "dependencies": {
    "@agm/core": "1.0.0-beta.2",
    "@angular/animations": "6.0.7",
    "@angular/cdk": "^6.3.2",
    "@angular/common": "6.0.7",
    "@angular/compiler": "6.0.7",
    "@angular/core": "6.0.7",
    "@angular/forms": "6.0.7",
    "@angular/http": "6.0.7",
    "@angular/material": "^6.3.2",
    "@angular/material-moment-adapter": "^7.1.1",
    "@angular/platform-browser": "6.0.7",
    "@angular/platform-browser-dynamic": "6.0.7",
    "@angular/platform-server": "6.0.7",
    "@angular/router": "6.0.7",
    "@auth0/angular-jwt": "1.2.0",
    "ajv": "6.4.0",
    "apollo-angular": "^1.1.2",
    "apollo-angular-link-http": "^1.1.0",
    "apollo-cache-inmemory": "^1.2.1",
    "apollo-client": "^2.3.5",
    "apollo-client-rxjs": "^0.6.0-rc.2",
    "apollo-link-context": "^1.0.8",
    "apollo-link-state": "^0.4.1",
    "arrive": "2.4.1",
    "await-to-js": "^2.0.1",
    "bootstrap": "4.1.2",
    "bootstrap-material-design": "4.1.1",
    "bootstrap-notify": "3.1.3",
    "chartist": "0.11.0",
    "classlist.js": "1.1.20150312",
    "core-js": "2.4.1",
    "googleapis": "28.1.0",
    "graphql": "^0.13.2",
    "graphql-tag": "^2.9.2",
    "graphql-tools": "^3.0.2",
    "hammerjs": "2.0.8",
    "jquery": "3.2.1",
    "lodash": "^4.17.11",
    "moment": "2.19.3",
    "ng2-file-upload": "^1.3.0",
    "ngx-papaparse": "3",
    "perfect-scrollbar": "1.1.0",
    "popper.js": "1.14.3",
    "rxjs": "^6.2.1",
    "rxjs-compat": "^6.2.1",
    "web-animations-js": "2.3.1",
    "zone.js": "0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.6.8",
    "@angular/cli": "^6.0.8",
    "@angular/compiler-cli": "6.0.7",
    "@angular/language-service": "6.0.7",
    "@types/bootstrap": "3.3.32",
    "@types/chartist": "0.9.34",
    "@types/googlemaps": "^3.30.8",
    "@types/jest": "^23.0.0",
    "@types/jquery": "1.10.31",
    "@types/node": "6.0.73",
    "casual": "^1.5.19",
    "codecov": "^3.0.2",
    "codelyzer": "4.2.1",
    "eyes.images": "^0.0.52",
    "fs-readfile-promise": "^3.0.1",
    "global": "^4.3.2",
    "graphql-cli": "^2.16.2",
    "husky": "^1.1.1",
    "jasmine-core": "~2.8.0",
    "jasmine-spec-reporter": "4.2.1",
    "jest": "^23.1.0",
    "jest-localstorage-mock": "^2.2.0",
    "jest-preset-angular": "^5.2.2",
    "jest-zone-patch": "^0.0.8",
    "json-graphql-server": "^2.1.1",
    "npx": "^10.2.0",
    "prettier": "^1.14.2",
    "pretty-quick": "^1.6.0",
    "protractor": "5.3.1",
    "shallow-render": "^6.0.3",
    "stryker": "^0.24.1",
    "stryker-api": "^0.17.2",
    "stryker-html-reporter": "^0.14.2",
    "stryker-jest-runner": "^0.7.0",
    "stryker-typescript": "^0.11.2",
    "stryker-webpack": "^0.0.4",
    "testcafe": "^0.20.4",
    "testcafe-angular-selectors": "^0.3.0",
    "testcafe-live": "^0.1.3",
    "ts-node": "5.0.1",
    "tslint": "5.10.0",
    "typescript": "^2.7.2",
    "uuid": "^3.3.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn format:fix",
      "pre-push": "yarn tslint"
    }
  },
  "jest": {
    "testURL": "http://localhost",
    "preset": "jest-preset-angular",
    "setupTestFrameworkScriptFile": "<rootDir>/src/setupJest.ts",
    "roots": [
      "src/app"
    ],
    "collectCoverageFrom": [
      "**/*.ts",
      "!**/*.module.ts",
      "!**/app.routing.ts",
      "!**/*.d.ts",
      "!src/setupJest.ts",
      "!src/app/models/*",
      "!**/*.tmp.ts",
      "!**/*.dialog.ts",
      "!**/login.component.ts",
      "!**/jwt.interceptor.ts",
      "!**/dashboard.component.ts",
      "!**/*.graphql.service.ts",
      "!src/app/apollo/state/apollo-config.factory.ts",
      "!src/app/apollo/state/resolvers/*"
    ]
  }
}
